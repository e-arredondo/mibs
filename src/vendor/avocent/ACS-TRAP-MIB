ACS-TRAP-MIB DEFINITIONS ::= BEGIN

IMPORTS
  enterprises, Integer32, MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE
    FROM SNMPv2-SMI
  OBJECT-GROUP, NOTIFICATION-GROUP FROM SNMPv2-CONF;

--------------------------------------------------------------------------------
-- Object Groupings
-- Enterpriseoid 10418
-- Familyoid 26
-- TrapOid 0
-- Productoid 1
-- Managementoid 2
-- TrapObjectoid 6
--------------------------------------------------------------------------------

acs    MODULE-IDENTITY
        LAST-UPDATED    "202410010000Z"
        ORGANIZATION    "Vertiv Co"
        CONTACT-INFO    "e-mail : support.avocent@vertivco.com"
        DESCRIPTION
        "This module defines the TRAP MIB extensions to
         acs products."
        REVISION        "202410010000Z"
        DESCRIPTION     "Added traps for port auto discovery."
        REVISION        "202405010000Z"
        DESCRIPTION     "Added traps for power device inlet changes."
        REVISION        "202309060000Z"
        DESCRIPTION     "Added traps for modem status, power device inlet alarm."
        REVISION        "202106240000Z"
        DESCRIPTION     "Updated to support SNMPv2/v3 traps."
        REVISION        "201708150000Z"
        DESCRIPTION     "Initial version"
::= { avocent 26 }

avocent                 OBJECT IDENTIFIER ::= { enterprises 10418 }

acsTrap                 OBJECT IDENTIFIER ::= { acs 0 }
acsProducts             OBJECT IDENTIFIER ::= { acs 1 }
acsManagement           OBJECT IDENTIFIER ::= { acs 2 }
acsTrapObject           OBJECT IDENTIFIER ::= { acsManagement 6 }
acsconformance          OBJECT IDENTIFIER ::= { acs 15 }
acsObjectGroups         OBJECT IDENTIFIER ::= { acsconformance 1 }

