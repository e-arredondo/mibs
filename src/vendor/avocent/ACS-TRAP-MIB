-- Appliance Management Traps
avctAcsRebootStartedTrap NOTIFICATION-TYPE
    OBJECTS   {
                  acsTrapObjectUserName
                }
    STATUS current
    DESCRIPTION
    "ACS8000 is in process of rebooting.
     The name of the user who issued the command is contained
     in acsTrapObjectUserName."
    --#SUMMARY     "The appliance is rebooting. Command issued by user: %s."
    --#ARGUMENTS   { 0 }
    --#SEVERITY    INFORMATIONAL
    --#CATEGORY    "Status Events"
    --NAME      "Appliance Rebooting"
    --SHORT     "appliance rebooting"
    --EVENT evt_reboot
    --% TrapTypeEnum=15
::= { acsTrap 1 }

