ACOS-GSLB-SERVICE-IP-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		DisplayString, PhysAddress
		FROM SNMPv2-TC
	InetAddressType
		FROM INET-ADDRESS-MIB
	acosSchema
		FROM A10-AX-MIB
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Integer32, Counter64, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;
		gslbServiceIpModule MODULE-IDENTITY
				LAST-UPDATED	"200705071327Z"
				ORGANIZATION	"A10 Networks, Inc."
				CONTACT-INFO	"Address: A10 Networks, Inc.
							  2300 Orchard Parkway
							  San Jose, CA 95131
						 Phone:   +1-888-822-7210 (USA/Canada)
						          +1-408-325-8676 (International)
						 E-mail:  support@A10Networks.com"

				DESCRIPTION	 "gslbServiceIpModule OID for ACOS"
				::=  {  acosSchema 424 }

serviceIp		 OBJECT IDENTIFIER ::= { gslbServiceIpModule 1}

serviceIpStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF ServiceIpStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "Service IP from object 'gslb.service-ip'"
	::= {serviceIp 1}

serviceIpStatsEntry OBJECT-TYPE
	SYNTAX ServiceIpStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The serviceIpStats stats entry"
	INDEX {
		serviceIpStatsSlotId,
		serviceIpStatsNodeName
	}
	::= { serviceIpStatsTable 1 }

ServiceIpStatsEntry ::=
	SEQUENCE {
		serviceIpStatsSlotId Integer32,
		serviceIpStatsNodeName DisplayString,
		serviceIpStatsHits	 CounterBasedGauge64,
		serviceIpStatsRecent	 CounterBasedGauge64
	}

