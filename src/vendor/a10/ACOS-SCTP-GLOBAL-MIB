ACOS-SCTP-GLOBAL-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		DisplayString, PhysAddress
		FROM SNMPv2-TC
	InetAddressType
		FROM INET-ADDRESS-MIB
	acosSchema
		FROM A10-AX-MIB
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Integer32, Counter64, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;
		sctpGlobalModule MODULE-IDENTITY
				LAST-UPDATED	"200705071327Z"
				ORGANIZATION	"A10 Networks, Inc."
				CONTACT-INFO	"Address: A10 Networks, Inc.
							  2300 Orchard Parkway
							  San Jose, CA 95131
						 Phone:   +1-888-822-7210 (USA/Canada)
						          +1-408-325-8676 (International)
						 E-mail:  support@A10Networks.com"

				DESCRIPTION	 "sctpGlobalModule OID for ACOS"
				::=  {  acosSchema 495 }

global		 OBJECT IDENTIFIER ::= { sctpGlobalModule 1}

globalStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF GlobalStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "SCTP Statistics from object 'sctp.global'"
	::= {global 1}

globalStatsEntry OBJECT-TYPE
	SYNTAX GlobalStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The globalStats stats entry"
	INDEX {
		globalStatsSlotId
	}
	::= { globalStatsTable 1 }

GlobalStatsEntry ::=
	SEQUENCE {
		globalStatsSlotId Integer32,
		globalStatsSctpStaticNatSessionCreated	 CounterBasedGauge64,
		globalStatsSctpStaticNatSessionDeleted	 CounterBasedGauge64,
		globalStatsSctpFwSessionCreated	 CounterBasedGauge64,
		globalStatsSctpFwSessionDeleted	 CounterBasedGauge64,
		globalStatsPktErrDrop	 CounterBasedGauge64,
		globalStatsBadCsum	 CounterBasedGauge64,
		globalStatsBadPayloadDrop	 CounterBasedGauge64,
		globalStatsBadAlignmentDrop	 CounterBasedGauge64,
		globalStatsOosPktDrop	 CounterBasedGauge64,
		globalStatsMaxMultiHomeDrop	 CounterBasedGauge64,
		globalStatsMultiHomeRemoveIpSkip	 CounterBasedGauge64,
		globalStatsMultiHomeAddrNotFoundDrop	 CounterBasedGauge64,
		globalStatsStaticNatCfgNotFound	 CounterBasedGauge64,
		globalStatsCfgErrDrop	 CounterBasedGauge64,
		globalStatsVrrpStandbyDrop	 CounterBasedGauge64,
		globalStatsInvalidFragChunkDrop	 CounterBasedGauge64,
		globalStatsDisallowedChunkFiltered	 CounterBasedGauge64,
		globalStatsDisallowedPktDrop	 CounterBasedGauge64,
		globalStatsRateLimitDrop	 CounterBasedGauge64,
		globalStatsSbySessionCreated	 CounterBasedGauge64,
		globalStatsSbySessionCreateFail	 CounterBasedGauge64,
		globalStatsSbySessionUpdated	 CounterBasedGauge64,
		globalStatsSbySessionUpdateFail	 CounterBasedGauge64,
		globalStatsSbyStaticNatCfgNotFound	 CounterBasedGauge64,
		globalStatsSctpOutOfSystemMemory	 CounterBasedGauge64,
		globalStatsConnExtSizeMax	 CounterBasedGauge64,
		globalStatsBadCsumShadow	 CounterBasedGauge64,
		globalStatsBadPayloadDropShadow	 CounterBasedGauge64,
		globalStatsBadAlignmentDropShadow	 CounterBasedGauge64,
		globalStatsSctpChunkTypeInit	 CounterBasedGauge64,
		globalStatsSctpChunkTypeInitAck	 CounterBasedGauge64,
		globalStatsSctpChunkTypeCookieEcho	 CounterBasedGauge64,
		globalStatsSctpChunkTypeCookieAck	 CounterBasedGauge64,
		globalStatsSctpChunkTypeSack	 CounterBasedGauge64,
		globalStatsSctpChunkTypeAsconf	 CounterBasedGauge64,
		globalStatsSctpChunkTypeAsconfAck	 CounterBasedGauge64,
		globalStatsSctpChunkTypeData	 CounterBasedGauge64,
		globalStatsSctpChunkTypeAbort	 CounterBasedGauge64,
		globalStatsSctpChunkTypeShutdown	 CounterBasedGauge64,
		globalStatsSctpChunkTypeShutdownAck	 CounterBasedGauge64,
		globalStatsSctpChunkTypeShutdownComplete	 CounterBasedGauge64,
		globalStatsSctpChunkTypeErrorOp	 CounterBasedGauge64,
		globalStatsSctpChunkTypeHeartbeat	 CounterBasedGauge64,
		globalStatsSctpChunkTypeHeartbeatAck	 CounterBasedGauge64,
		globalStatsSctpChunkTypeOther	 CounterBasedGauge64,
		globalStatsSctpHeartbeatNoTuple	 CounterBasedGauge64,
		globalStatsSctpDataNoTuple	 CounterBasedGauge64,
		globalStatsSctpDataNoExtMatch	 CounterBasedGauge64,
		globalStatsSctpChunkTypeInitDrop	 CounterBasedGauge64,
		globalStatsSctpChunkTypeInitAckDrop	 CounterBasedGauge64,
		globalStatsSctpChunkTypeShutdownCompleteDrop	 CounterBasedGauge64,
		globalStatsSctpChunkTypeAbortDataDrop	 CounterBasedGauge64,
		globalStatsSctpChunkHeartBeatClubbed	 CounterBasedGauge64,
		globalStatsSctpRetxInitAckDrop	 CounterBasedGauge64,
		globalStatsSctpRouteErrHeartbeatDrop	 CounterBasedGauge64,
		globalStatsSctpRerouteFailover	 CounterBasedGauge64,
		globalStatsSctpRouteErrDrop	 CounterBasedGauge64,
		globalStatsSctpNoExtMatch	 CounterBasedGauge64,
		globalStatsSctpRetxInitAck	 CounterBasedGauge64,
		globalStatsSctpRetxInitDrop	 CounterBasedGauge64,
		globalStatsSctpRetxInit	 CounterBasedGauge64,
