		globalStatsSctpInitVtagZeroDrop	 CounterBasedGauge64,
		globalStatsPktLenErrDrop	 CounterBasedGauge64,
		globalStatsPktChunkLenErrDrop	 CounterBasedGauge64,
		globalStatsPktAsconfParamLenErrDrop	 CounterBasedGauge64
	}

