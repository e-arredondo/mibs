statStatsSlaveCfgUpdNotifErr OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "slave_cfg_upd_notif_err"
	::= { statStatsEntry 15 }

statStatsSlaveCfgUpdResultErr OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "slave_cfg_upd_result_err"
	::= { statStatsEntry 16 }

END

