ACOS-SLB-SSL-FORWARD-PROXY-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		DisplayString, PhysAddress
		FROM SNMPv2-TC
	InetAddressType
		FROM INET-ADDRESS-MIB
	acosSchema
		FROM A10-AX-MIB
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Integer32, Counter64, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;
		slbSslForwardProxyModule MODULE-IDENTITY
				LAST-UPDATED	"200705071327Z"
				ORGANIZATION	"A10 Networks, Inc."
				CONTACT-INFO	"Address: A10 Networks, Inc.
							  2300 Orchard Parkway
							  San Jose, CA 95131
						 Phone:   +1-888-822-7210 (USA/Canada)
						          +1-408-325-8676 (International)
						 E-mail:  support@A10Networks.com"

				DESCRIPTION	 "slbSslForwardProxyModule OID for ACOS"
				::=  {  acosSchema 312 }

sslForwardProxy		 OBJECT IDENTIFIER ::= { slbSslForwardProxyModule 1}

sslForwardProxyStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF SslForwardProxyStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "SSL forward proxy stats info from object 'slb.ssl-forward-proxy'"
	::= {sslForwardProxy 1}

sslForwardProxyStatsEntry OBJECT-TYPE
	SYNTAX SslForwardProxyStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The sslForwardProxyStats stats entry"
	INDEX {
		sslForwardProxyStatsSlotId
	}
	::= { sslForwardProxyStatsTable 1 }

SslForwardProxyStatsEntry ::=
	SEQUENCE {
		sslForwardProxyStatsSlotId Integer32,
		sslForwardProxyStatsCertCreate	 CounterBasedGauge64,
		sslForwardProxyStatsCertExpr	 CounterBasedGauge64,
		sslForwardProxyStatsCertHit	 CounterBasedGauge64,
		sslForwardProxyStatsCertMiss	 CounterBasedGauge64,
		sslForwardProxyStatsConnBypass	 CounterBasedGauge64,
		sslForwardProxyStatsConnInspect	 CounterBasedGauge64,
		sslForwardProxyStatsBypassFailsafeSslSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassSniSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassClientAuthSessions	 CounterBasedGauge64,
		sslForwardProxyStatsFailedInSslHandshakes	 CounterBasedGauge64,
		sslForwardProxyStatsFailedInCryptoOperations	 CounterBasedGauge64,
		sslForwardProxyStatsFailedInTcp	 CounterBasedGauge64,
		sslForwardProxyStatsFailedInCertificateVerification	 CounterBasedGauge64,
		sslForwardProxyStatsFailedInCertificateSigning	 CounterBasedGauge64,
		sslForwardProxyStatsInvalidOcspStaplingResponse	 CounterBasedGauge64,
		sslForwardProxyStatsRevokedOcspResponse	 CounterBasedGauge64,
		sslForwardProxyStatsUnsupportedSslVersion	 CounterBasedGauge64,
		sslForwardProxyStatsCertificatesInCache	 CounterBasedGauge64,
		sslForwardProxyStatsConnectionsFailed	 CounterBasedGauge64,
		sslForwardProxyStatsAflexBypass	 CounterBasedGauge64,
		sslForwardProxyStatsBypassCertSubjectSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassCertIssuerSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassCertSanSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassNoSniSessions	 CounterBasedGauge64,
		sslForwardProxyStatsResetNoSniSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassEsniSessions	 CounterBasedGauge64,
		sslForwardProxyStatsDropEsniSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassUsernameSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassAdGroupSessions	 CounterBasedGauge64,
		sslForwardProxyStatsTotConnInBuff	 CounterBasedGauge64,
		sslForwardProxyStatsCurrConnInBuff	 CounterBasedGauge64,
		sslForwardProxyStatsAsyncConnTimeout	 CounterBasedGauge64,
		sslForwardProxyStatsAsyncConnLimitDrop	 CounterBasedGauge64,
		sslForwardProxyStatsCertInCache	 CounterBasedGauge64,
		sslForwardProxyStatsBypassClientIpSessions	 CounterBasedGauge64,
		sslForwardProxyStatsBypassServerIpSessions	 CounterBasedGauge64
	}

sslForwardProxyStatsSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { sslForwardProxyStatsEntry 1 }

