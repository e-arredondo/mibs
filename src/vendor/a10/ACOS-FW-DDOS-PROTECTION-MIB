ACOS-FW-DDOS-PROTECTION-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		DisplayString, PhysAddress
		FROM SNMPv2-TC
	InetAddressType
		FROM INET-ADDRESS-MIB
	acosSchema
		FROM A10-AX-MIB
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Integer32, Counter64, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;
		fwDdosProtectionModule MODULE-IDENTITY
				LAST-UPDATED	"200705071327Z"
				ORGANIZATION	"A10 Networks, Inc."
				CONTACT-INFO	"Address: A10 Networks, Inc.
							  2300 Orchard Parkway
							  San Jose, CA 95131
						 Phone:   +1-888-822-7210 (USA/Canada)
						          +1-408-325-8676 (International)
						 E-mail:  support@A10Networks.com"

				DESCRIPTION	 "fwDdosProtectionModule OID for ACOS"
				::=  {  acosSchema 491 }

ddosProtection		 OBJECT IDENTIFIER ::= { fwDdosProtectionModule 1}

ddosProtectionStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF DdosProtectionStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "Configure FW DDoS Protection from object 'fw.ddos-protection'"
	::= {ddosProtection 1}

ddosProtectionStatsEntry OBJECT-TYPE
	SYNTAX DdosProtectionStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The ddosProtectionStats stats entry"
	INDEX {
		ddosProtectionStatsSlotId
	}
	::= { ddosProtectionStatsTable 1 }

DdosProtectionStatsEntry ::=
	SEQUENCE {
		ddosProtectionStatsSlotId Integer32,
		ddosProtectionStatsDdosEntriesTooMany	 CounterBasedGauge64,
		ddosProtectionStatsDdosEntryAdded	 CounterBasedGauge64,
		ddosProtectionStatsDdosEntryRemoved	 CounterBasedGauge64,
		ddosProtectionStatsDdosEntryAddedToBgp	 CounterBasedGauge64,
		ddosProtectionStatsDdosEntryRemovedFromBgp	 CounterBasedGauge64,
		ddosProtectionStatsDdosEntryAddToBgpFailure	 CounterBasedGauge64,
		ddosProtectionStatsDdosEntryRemoveFromBgpFailure	 CounterBasedGauge64,
		ddosProtectionStatsDdosPacketDropped	 CounterBasedGauge64
	}

ddosProtectionStatsSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { ddosProtectionStatsEntry 1 }

ddosProtectionStatsDdosEntriesTooMany OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "ddos_entries_too_many"
	::= { ddosProtectionStatsEntry 2 }

ddosProtectionStatsDdosEntryAdded OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "ddos_entry_added"
	::= { ddosProtectionStatsEntry 3 }

ddosProtectionStatsDdosEntryRemoved OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "ddos_entry_removed"
	::= { ddosProtectionStatsEntry 4 }

