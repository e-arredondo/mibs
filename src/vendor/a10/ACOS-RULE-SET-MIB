ACOS-RULE-SET-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		DisplayString, PhysAddress
		FROM SNMPv2-TC
	InetAddressType
		FROM INET-ADDRESS-MIB
	acosSchema
		FROM A10-AX-MIB
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Integer32, Counter64, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;
		ruleSetModule MODULE-IDENTITY
				LAST-UPDATED	"200705071327Z"
				ORGANIZATION	"A10 Networks, Inc."
				CONTACT-INFO	"Address: A10 Networks, Inc.
							  2300 Orchard Parkway
							  San Jose, CA 95131
						 Phone:   +1-888-822-7210 (USA/Canada)
						          +1-408-325-8676 (International)
						 E-mail:  support@A10Networks.com"

				DESCRIPTION	 "ruleSetModule OID for ACOS"
				::=  {  acosSchema 470 }

ruleSet		 OBJECT IDENTIFIER ::= { ruleSetModule 1}

ruleSetStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF RuleSetStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "Configure Security policy Rule Set from object 'rule-set'"
	::= {ruleSet 1}

ruleSetStatsEntry OBJECT-TYPE
	SYNTAX RuleSetStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The ruleSetStats stats entry"
	INDEX {
		ruleSetStatsSlotId,
		ruleSetStatsName
	}
	::= { ruleSetStatsTable 1 }

RuleSetStatsEntry ::=
	SEQUENCE {
		ruleSetStatsSlotId Integer32,
		ruleSetStatsName DisplayString,
		ruleSetStatsUnmatchedDrops	 CounterBasedGauge64,
		ruleSetStatsPermit	 CounterBasedGauge64,
		ruleSetStatsDeny	 CounterBasedGauge64,
		ruleSetStatsReset	 CounterBasedGauge64
	}

ruleSetStatsSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { ruleSetStatsEntry 1 }

ruleSetStatsName OBJECT-TYPE
	 SYNTAX DisplayString
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "name"
	::= { ruleSetStatsEntry 2 }

ruleSetStatsUnmatchedDrops OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "unmatched-drops"
	::= { ruleSetStatsEntry 3 }

