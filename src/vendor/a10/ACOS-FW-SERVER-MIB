	::= { portOperEntry 8 }

portOperHaGroupId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "ha_group_id"
	::= { portOperEntry 9 }

portOperPortsConsumed OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "ports_consumed"
	::= { portOperEntry 10 }

portOperPortsConsumedTotal OBJECT-TYPE
