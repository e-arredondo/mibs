ACOS-FW-SERVER-MIB DEFINITIONS ::= BEGIN
	IMPORTS
		DisplayString, PhysAddress
		FROM SNMPv2-TC
	InetAddressType
		FROM INET-ADDRESS-MIB
	acosSchema
		FROM A10-AX-MIB
	MODULE-IDENTITY, OBJECT-TYPE, Counter32, Integer32, Counter64, OBJECT-IDENTITY, NOTIFICATION-TYPE
		FROM SNMPv2-SMI;
		fwServerModule MODULE-IDENTITY
				LAST-UPDATED	"200705071327Z"
				ORGANIZATION	"A10 Networks, Inc."
				CONTACT-INFO	"Address: A10 Networks, Inc.
							  2300 Orchard Parkway
							  San Jose, CA 95131
						 Phone:   +1-888-822-7210 (USA/Canada)
						          +1-408-325-8676 (International)
						 E-mail:  support@A10Networks.com"

				DESCRIPTION	 "fwServerModule OID for ACOS"
				::=  {  acosSchema 478 }

server		 OBJECT IDENTIFIER ::= { fwServerModule 1}

serverStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF ServerStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "Firewall logging Server from object 'fw.server'"
	::= {server 1}

serverStatsEntry OBJECT-TYPE
	SYNTAX ServerStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The serverStats stats entry"
	INDEX {
		serverStatsSlotId,
		serverStatsName
	}
	::= { serverStatsTable 1 }

ServerStatsEntry ::=
	SEQUENCE {
		serverStatsSlotId Integer32,
		serverStatsName DisplayString,
		serverStatsCurrConn	 CounterBasedGauge64,
		serverStatsTotalConn	 CounterBasedGauge64,
		serverStatsFwdPkt	 CounterBasedGauge64,
		serverStatsRevPkt	 CounterBasedGauge64,
		serverStatsPeakConn	 CounterBasedGauge64
	}

serverStatsSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { serverStatsEntry 1 }

serverStatsName OBJECT-TYPE
	 SYNTAX DisplayString
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "name"
	::= { serverStatsEntry 2 }

serverStatsCurrConn OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "curr-conn"
	::= { serverStatsEntry 3 }

serverStatsTotalConn OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "total-conn"
	::= { serverStatsEntry 4 }

serverStatsFwdPkt OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "fwd-pkt"
	::= { serverStatsEntry 5 }

serverStatsRevPkt OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "rev-pkt"
	::= { serverStatsEntry 6 }

serverStatsPeakConn OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "peak-conn"
	::= { serverStatsEntry 7 }

serverOperTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF ServerOperEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
		 "Firewall logging Server from object 'fw.server'"
	::= {server 2}

serverOperEntry OBJECT-TYPE
	SYNTAX ServerOperEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The serverOper entry"
	INDEX {
		serverOperSlotId,
		serverOperName
	}
	::= { serverOperTable 1 }

ServerOperEntry ::=
	SEQUENCE {
		serverOperSlotId Integer32,
		serverOperName DisplayString,
		serverOperState	 DisplayString
	}

serverOperSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { serverOperEntry 1 }

serverOperName OBJECT-TYPE
	 SYNTAX DisplayString
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "name"
	::= { serverOperEntry 2 }

serverOperState OBJECT-TYPE
	 SYNTAX DisplayString
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 "state"
	::= { serverOperEntry 3 }

port		 OBJECT IDENTIFIER ::= { server 3}

portStatsTable OBJECT-TYPE
	 SYNTAX SEQUENCE OF PortStatsEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
	 "Real Server Port from object 'fw.server.port'"
	::= {port 1}

portStatsEntry OBJECT-TYPE
	SYNTAX PortStatsEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The portStats stats entry"
	INDEX {
		portStatsSlotId,
		portStatsName,
		portStatsPort,
		portStatsProtocol
	}
	::= { portStatsTable 1 }

PortStatsEntry ::=
	SEQUENCE {
		portStatsSlotId Integer32,
		portStatsName DisplayString,
		portStatsPort Integer32,
		portStatsProtocol DisplayString,
		portStatsCurrConn	 CounterBasedGauge64,
		portStatsCurrReq	 CounterBasedGauge64,
		portStatsTotalReq	 CounterBasedGauge64,
		portStatsTotalReqSucc	 CounterBasedGauge64,
		portStatsTotalFwdBytes	 CounterBasedGauge64,
		portStatsTotalFwdPkts	 CounterBasedGauge64,
		portStatsTotalRevBytes	 CounterBasedGauge64,
		portStatsTotalRevPkts	 CounterBasedGauge64,
		portStatsTotalConn	 CounterBasedGauge64,
		portStatsLastTotalConn	 CounterBasedGauge64,
		portStatsPeakConn	 CounterBasedGauge64,
		portStatsEsResp200	 CounterBasedGauge64,
		portStatsEsResp300	 CounterBasedGauge64,
		portStatsEsResp400	 CounterBasedGauge64,
		portStatsEsResp500	 CounterBasedGauge64,
		portStatsEsRespOther	 CounterBasedGauge64,
		portStatsEsReqCount	 CounterBasedGauge64,
		portStatsEsRespCount	 CounterBasedGauge64,
		portStatsEsRespInvalidHttp	 CounterBasedGauge64,
		portStatsTotalRevPktsInspected	 CounterBasedGauge64,
		portStatsTotalRevPktsInspectedGoodStatusCode	 CounterBasedGauge64,
		portStatsResponseTime	 CounterBasedGauge64,
		portStatsFastestRspTime	 CounterBasedGauge64,
		portStatsSlowestRspTime	 CounterBasedGauge64
	}

portStatsSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { portStatsEntry 1 }

