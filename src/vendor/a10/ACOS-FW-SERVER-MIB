	 SYNTAX SEQUENCE OF PortOperEntry
	 MAX-ACCESS not-accessible
	 STATUS current
	 DESCRIPTION
		 "Real Server Port from object 'fw.server.port'"
	::= {port 2}

portOperEntry OBJECT-TYPE
	SYNTAX PortOperEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The portOper entry"
	INDEX {
		portOperSlotId,
		portOperName,
		portOperPort,
		portOperProtocol
	}
	::= { portOperTable 1 }

PortOperEntry ::=
	SEQUENCE {
		portOperSlotId Integer32,
		portOperName DisplayString,
		portOperPort Integer32,
		portOperProtocol DisplayString,
		portOperState	 DisplayString,
		portOperIp	 DisplayString,
		portOperIpv6	 DisplayString,
		portOperVrid	 Integer32,
		portOperHaGroupId	 Integer32,
		portOperPortsConsumed	 Integer32,
		portOperPortsConsumedTotal	 Integer32,
		portOperPortsFreedTotal	 Integer32,
		portOperAllocFailed	 Integer32
	}

portOperSlotId OBJECT-TYPE
	 SYNTAX Integer32
	 MAX-ACCESS read-only
	 STATUS current
	 DESCRIPTION
		 ""
	::= { portOperEntry 1 }

portOperName OBJECT-TYPE
	 SYNTAX DisplayString
	 MAX-ACCESS read-only
	 STATUS current
