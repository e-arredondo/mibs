apDosThresholdCountersObjects OBJECT-GROUP
        OBJECTS {
          apDosTrustedMinorCounter,
          apDosTrustedMajorCounter,
          apDosTrustedCriticalCounter,
          apDosUntrustedMinorCounter,
          apDosUntrustedMajorCounter,
          apDosUntrustedCriticalCounter,
          apDosArpMinorCounter,
          apDosArpMajorCounter,
          apDosArpCriticalCounter
        }
        STATUS current
        DESCRIPTION "A collection of objects which provide dos threshold counters"
           ::= {apAppsModule 5}

apDosTrustedMinorCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Minor threshold
                 percentage of trusted traffic is crossed."
         ::= {apDosThresholdCountersObjects 1}

apDosTrustedMajorCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Major threshold
                 percentage of trusted traffic is crossed."
         ::= {apDosThresholdCountersObjects 2}

apDosTrustedCriticalCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Critical threshold
                 percentage of trusted traffic is crossed."
         ::= {apDosThresholdCountersObjects 3}

apDosUntrustedMinorCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Minor threshold
                 percentage of untrusted traffic is crossed."
         ::= {apDosThresholdCountersObjects 4}

apDosUntrustedMajorCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Major threshold
                 percentage of untrusted traffic is crossed."
         ::= {apDosThresholdCountersObjects 5}

apDosUntrustedCriticalCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Critical threshold
                 percentage of untrusted traffic is crossed."
         ::= {apDosThresholdCountersObjects 6}

apDosArpMinorCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Minor threshold
                 percentage of ARP traffic is crossed."
         ::= {apDosThresholdCountersObjects 7}

apDosArpMajorCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Major threshold
                 percentage of ARP traffic is crossed."
         ::= {apDosThresholdCountersObjects 8}

apDosArpCriticalCounter OBJECT-TYPE
        SYNTAX                    Counter64
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
                "The number of times, Critical threshold
                 percentage of ARP traffic is crossed."
         ::= {apDosThresholdCountersObjects 9}

-- DOS threshold Notification Objects

apDosThresholdNotificationObjects OBJECT IDENTIFIER ::= { apAppsModule 6 }

apDosThresholdTraffic OBJECT-TYPE
        SYNTAX                  ApDosThresholdTrafficType
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "1 = trusted, 2 = untrusted , 3 = ARP"
        ::= { apDosThresholdNotificationObjects 1 }


apDosThresholdMinorCrossed OBJECT-TYPE
        SYNTAX                  ApDosThresholdCrossState
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "0 = threshold not crossed, 1 = threshold crossed"
        ::= { apDosThresholdNotificationObjects 2 }

apDosThresholdMajorCrossed OBJECT-TYPE
        SYNTAX                  ApDosThresholdCrossState
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "0 = threshold not crossed, 1 = threshold crossed"
        ::= { apDosThresholdNotificationObjects 3 }

apDosThresholdCriticalCrossed OBJECT-TYPE
        SYNTAX                  ApDosThresholdCrossState
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "0 = threshold not crossed, 1 = threshold crossed"
        ::= { apDosThresholdNotificationObjects 4 }

apDosThresholdMinorCounter OBJECT-TYPE
        SYNTAX                  Counter64
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "Counters populated, when the specified traffic 
         crossed the configured minor threshold percentage"
        ::= { apDosThresholdNotificationObjects 5 }

apDosThresholdMajorCounter OBJECT-TYPE
        SYNTAX                  Counter64
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "Counters populated, when the specified traffic
         crossed the configured major threshold percentage"
        ::= { apDosThresholdNotificationObjects 6 }

apDosThresholdCriticalCounter OBJECT-TYPE
        SYNTAX                  Counter64
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
        "Counters populated, when the specified traffic
         crossed the configured critical threshold percentage"
        ::= { apDosThresholdNotificationObjects 7 }


-- DOS threshold Notifications

apDosThresholdNotificationsPrefix   OBJECT IDENTIFIER
    ::= { apAppsModule 7 }

apDosThresholdNotifications OBJECT IDENTIFIER
     ::= { apDosThresholdNotificationsPrefix 0 }

apDosThresholdCrossTrap       NOTIFICATION-TYPE
          OBJECTS {
         apDosThresholdTraffic,
         apDosThresholdMinorCrossed,
         apDosThresholdMajorCrossed,
         apDosThresholdCriticalCrossed,
         apDosThresholdMinorCounter,
         apDosThresholdMajorCounter,
         apDosThresholdCriticalCounter
                }
        STATUS  current
        DESCRIPTION
            "The trap will be generated when the specified
             traffic crosses the configured DOS thresholds."
        ::= { apDosThresholdNotifications 1 }

apDosThresholdClearTrap       NOTIFICATION-TYPE
        OBJECTS {
         apDosThresholdTraffic,
         apDosThresholdMinorCounter,
         apDosThresholdMajorCounter,
         apDosThresholdCriticalCounter
                }
        STATUS  current
        DESCRIPTION
            "The clear trap will be generated when the specified
             traffic had fallen back from the all configured DOS thresholds"
        ::= { apDosThresholdNotifications 2 }

apDosThresholdCountersGroup OBJECT-GROUP
         OBJECTS {
		 apDosTrustedMinorCounter,
		 apDosTrustedMajorCounter,
		 apDosTrustedCriticalCounter,
		 apDosUntrustedMinorCounter,
		 apDosUntrustedMajorCounter,
		 apDosUntrustedCriticalCounter,
		 apDosArpMinorCounter,
		 apDosArpMajorCounter,
		 apDosArpCriticalCounter
                 }
         STATUS          current
         DESCRIPTION
                "Object to monitor Dos threshold counters"
         ::= { apAppsObjectGroups 4 }

apDosThresholdNotificationsGroup NOTIFICATION-GROUP
        NOTIFICATIONS{
            apDosThresholdCrossTrap,
            apDosThresholdClearTrap
        }
        STATUS          obsolete
        DESCRIPTION
            "Traps to monitor DOS threshold"
        ::= { apAppsNotificationGroups 5 }

apAppsStirMIBObjects OBJECT IDENTIFIER ::= {apAppsMIBTabularObjects 4}

apAppsStirServerTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF ApAppsStirServerEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "A read-only table to hold the STIR server names."
        ::= { apAppsStirMIBObjects 1 }

apAppsStirServerEntry OBJECT-TYPE
        SYNTAX  	ApAppsStirServerEntry
        MAX-ACCESS      not-accessible
        STATUS  	current
        DESCRIPTION
                "STIR server entry."
        INDEX	{ apStirServerIndex }
        ::= { apAppsStirServerTable 1 }


ApAppsStirServerEntry ::=
        SEQUENCE {
	        apStirServerIndex		Integer32,
                apStirServerName		DisplayString
	}
	
apStirServerIndex		OBJECT-TYPE
        SYNTAX			Integer32 (0..2147483647)
        MAX-ACCESS		not-accessible
        STATUS			current
        DESCRIPTION
	    "An integer for the sole purpose of indexing the STIR server entry."
