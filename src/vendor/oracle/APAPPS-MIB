    STATUS                      current
    DESCRIPTION
            "An integer for the sole purpose of indexing the NSEP R-value/Dialed Number entry."
    ::= { apAppsNSEPRealmRvalueDNEntry 1 }

apNSEPRvalueDNName              OBJECT-TYPE
    SYNTAX                      DisplayString (SIZE (0..255))
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION
            "The NSEP R-value/Dialed Number name"
    ::= { apAppsNSEPRealmRvalueDNEntry 2 }

apNSEPRealmRvalueDNStatsTable   OBJECT-TYPE
    SYNTAX                      SEQUENCE OF ApAppsNSEPRealmRvalueDNStatsEntry
    MAX-ACCESS                  not-accessible
    STATUS                      current
    DESCRIPTION
            "A read-only table to hold the NSEP Realm Stats of R-value/Dialed Number."
    ::= { apAppsNSEPRealmMIBObjects 3 }

apAppsNSEPRealmRvalueDNStatsEntry       OBJECT-TYPE
    SYNTAX                              ApAppsNSEPRealmRvalueDNStatsEntry
    MAX-ACCESS                          not-accessible
    STATUS                              current
    DESCRIPTION
            "NSEP Realm Stats Entry"
    INDEX   {   apNSEPRealmIndex,
                apNSEPRealmRvalueDNIndex,
                apNSEPRealmStatsType
            }
    ::= { apNSEPRealmRvalueDNStatsTable 1 }

ApAppsNSEPRealmRvalueDNStatsEntry ::=
    SEQUENCE {
            apNSEPStatsRealmIndex       Integer32,
            apNSEPStatsRvalueDNIndex    ApNSEPRealmRvalueDNIndex,
            apNSEPRealmStatsType        ApNSEPRealmStatsType,
            apNSEPRealmStats            Gauge32
}

apNSEPStatsRealmIndex           OBJECT-TYPE
    SYNTAX                      Integer32
    MAX-ACCESS                  not-accessible
    STATUS                      current
    DESCRIPTION
            "The NSEP Realm Index"
    ::= { apAppsNSEPRealmRvalueDNStatsEntry 1 }

apNSEPStatsRvalueDNIndex        OBJECT-TYPE
    SYNTAX                      ApNSEPRealmRvalueDNIndex
    MAX-ACCESS                  not-accessible
    STATUS                      current
    DESCRIPTION
            "The NSEP Realm R-value/Dialed Number Index"
    ::= { apAppsNSEPRealmRvalueDNStatsEntry 2 }

apNSEPRealmStatsType            OBJECT-TYPE
    SYNTAX                      ApNSEPRealmStatsType
    MAX-ACCESS                  not-accessible
    STATUS                      current
    DESCRIPTION
            "The NSEP Stats Index"
    ::= { apAppsNSEPRealmRvalueDNStatsEntry 3 }

apNSEPRealmStats                OBJECT-TYPE
    SYNTAX                      Gauge32
    MAX-ACCESS                  read-only
    STATUS                      current
    DESCRIPTION
            "The NSEPRealm Stats"
    ::= { apAppsNSEPRealmRvalueDNStatsEntry 4 }

apNSEPRealmStatsGroup           OBJECT-GROUP
    OBJECTS{apNSEPRealmName,
            apNSEPRvalueDNName,
            apNSEPRealmStats
    }
    STATUS                      current
    DESCRIPTION
            "Report the name and stats of NSEP ."
    ::= { apAppsObjectGroups 12 }
END
