APAPPS-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        Gauge32, Integer32, Counter32, Counter64
                FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, DisplayString, TruthValue
                FROM SNMPv2-TC
        MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
                FROM SNMPv2-CONF
        InetAddress, InetAddressType
	    FROM INET-ADDRESS-MIB
	acmepacketMgmt
                FROM ACMEPACKET-SMI
        ApHardwareModuleFamily, ApRedundancyState, ApPhyPortType,
	ApPresence, ApServerStatus, ApCommMonitorState, ApAclType, 
	ApCounterStatsType, ApStirStatsType,
	ApMsrpKpiStatsCounterType, ApMsrpKpiStatsType,
        ApDosThresholdTrafficType, ApDosThresholdCrossState, ApPercentage,
        ApNSEPRealmRvalueDNIndex, ApNSEPRealmStatsType
	    FROM ACMEPACKET-TC;

